{
  "RabbitMqSettings": {
    "HostName": "rabbitmq",
    "Port": 5672,
    "Exchange": "oms",
    "OrderCreated": {
      "Queue": "oms.order.created",
      "RoutingKey": "order.created",
      "BatchSize": 100,
      "BatchTimeoutSeconds": 1,
      "DeadLetter": {
        "Dlx": "oms.order.dlx",
        "Dlq": "oms.order.created.dlq",
        "RoutingKey": "order.created"
      }
    },
    "OrderStatusChanged": {
      "Queue": "oms.order.status.changed",
      "RoutingKey": "order.status.changed",
      "BatchSize": 100,
      "BatchTimeoutSeconds": 1,
      "DeadLetter": {
        "Dlx": "oms.order.dlx",
        "Dlq": "oms.order.status.changed.dlq",
        "RoutingKey": "order.status.changed"
      }
    },
    "PublicOrderCreated": {
      "Queue": "public.oms.order.created",
      "RoutingKey": "order.created",
      "BatchSize": 100,
      "BatchTimeoutSeconds": 1
    },
    "PublicOrderStatusChanged": {
      "Queue": "public.oms.order.status.changed",
      "RoutingKey": "order.status.changed",
      "BatchSize": 100,
      "BatchTimeoutSeconds": 1
    }
  },
  "HttpClient": {
    "Oms": {
      "BaseAddress": "http://webapi:8080"
    }
  }
}